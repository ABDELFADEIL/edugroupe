package tableauxUneDim;

public class Tp1 {

	public static void main(String[] args) {
	
		int [] tableauEntiers = {1,2,3,4,5,6,7,8,9,0};

	}
	
	
	public static int somme(int [] tabEnt) {
		
		int somme =0;
				
			for(int i=0;i<tabEnt.length;i++) {
				somme += tabEnt[i];
			}
		return somme;
	}

}
