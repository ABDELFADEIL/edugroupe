package tableauxUneDim;

public class Personne {
	//1	Attributs
	private int id;
	private String nom;
	private String prenom;
	private String email;
	//2-	Constructeurs
	public Personne() {
		
	}
	public Personne(int idPers) {
		this.id = idPers;
	}
	public Personne(int idPers,String nomPers) {
		this.id = idPers;
		this.nom =nomPers;
	}

	//3-	Getteurs ou accesseurs
	public int getId() {
		return this.id;
	}
}
